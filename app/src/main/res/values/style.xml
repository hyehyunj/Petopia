<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Contact Detail Alert Dialog-->

    <style name="dialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <!-- Toast -->
    <style name="toast_custom">
        <item name="stTextBold">true</item>
        <item name="stTextColor">@color/main_Sky_blue</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">@color/white</item>
        <item name="stStrokeWidth">3dp</item>
        <item name="stStrokeColor">@color/main_Sky_blue</item>
        <item name="stIconStart">@drawable/ic_launcher_foreground</item>
        <item name="stIconEnd">@drawable/ic_launcher_foreground</item>
        <item name="stLength">SHORT</item> LONG or SHORT
        <item name="stGravity">center</item> top or center
        <item name="stRadius">20dp</item>
    </style>



</resources>