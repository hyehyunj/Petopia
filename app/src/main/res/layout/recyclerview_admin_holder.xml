<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/admin_holder"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/admin_tv_title"
        style="@style/common_text_16_bold_dark_gray"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="@string/community_detail_hint_comment"
        android:text="신고합니다."
        app:layout_constraintEnd_toStartOf="@+id/admin_tv_reporter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/admin_tv_reporter"
        style="@style/point_text_20_gray"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="작성자"
        app:layout_constraintBottom_toBottomOf="@+id/admin_tv_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/admin_tv_title" />


</androidx.constraintlayout.widget.ConstraintLayout>